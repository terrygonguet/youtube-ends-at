{
	"manifest_version": 2,
	"name": "Youtube Ends At",
	"version": "1.0",

	"description": "Displays when a Youtube video will end",

	"icons": {
		"48": "icons/icon.svg",
		"96": "icons/icon.svg"
	},

	"content_scripts": [
		{
			"matches": ["*://www.youtube.com/*"],
			"js": ["scripts/content.js"]
		}
	],

	"browser_specific_settings": {
		"gecko": {
			"id": "youtube-ends-at@gonguet.com"
		}
	}
}
